<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <key>q00000014_00_14</key>
    <original xml:space="preserve">千の咆哮</original>
    <translation xml:space="preserve">The Roars of A Thousand</translation>
  </message>
  <message>
    <key>q00000014_00_14</key>
    <original xml:space="preserve">オーク軍の侵攻を退け神殿を守る</original>
  </message>
  <message>
    <key>q00000014_00_1003</key>
    <original xml:space="preserve">オーク軍を迎え撃つためハイデル平原へ向かう</original>
    <translation xml:space="preserve">Head out to Hidell Plains and fight the Orc forces.</translation>
  </message>
  <message>
    <key>q00000014_00_1004</key>
    <original xml:space="preserve">オーク軍先発部隊を掃討する</original>
    <translation xml:space="preserve">Eliminate the Orc advance party.</translation>
  </message>
  <message>
    <key>q00000014_00_1005</key>
    <original xml:space="preserve">オーク軍支援部隊を掃討する</original>
    <translation xml:space="preserve">Eliminate the Orc support troops.</translation>
  </message>
  <message>
    <key>q00000014_00_1006</key>
    <original xml:space="preserve">オーク軍本隊のいる場所に向かう</original>
    <translation xml:space="preserve">Head to where the Orc main force is located.</translation>
  </message>
  <message>
    <key>q00000014_00_1007</key>
    <original xml:space="preserve">オーク軍本隊を殲滅する</original>
    <translation xml:space="preserve">Eliminate the Orc main force.</translation>
  </message>
  <message>
    <key>q00000014_00_1008</key>
    <original xml:space="preserve">謁見の間のジョゼフに経緯を報告する</original>
    <translation xml:space="preserve">Report back to Joseph in the Audience Chamber.</translation>
  </message>
</resource>