<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <key>q00000013_00_13</key>
    <original xml:space="preserve">奪われた心</original>
	<translation xml:space="preserve">The Stolen Heart</translation>
  </message>
  <message>
    <key>q00000013_00_13</key>
    <original xml:space="preserve">さらわれたメイリーフを奪還する</original>
  </message>
  <message>
    <key>q00000013_00_998</key>
    <original xml:space="preserve">レオから情報を得る</original>
    <translation xml:space="preserve">Talk to Leo.</translation>
  </message>
  <message>
    <key>q00000013_00_1209</key>
    <original xml:space="preserve">目的地をレオから再確認する</original>
    <translation xml:space="preserve">Talk to Leo again.</translation>
  </message>
  <message>
    <key>q00000013_00_999</key>
    <original xml:space="preserve">グリッテン砦のスケルツォから情報を得る</original>
    <translation xml:space="preserve">Talk to Scherzo at Gritten Fort to obtain information.</translation>
  </message>
  <message>
    <key>q00000013_00_1548</key>
    <original xml:space="preserve">禊の神殿へ向かう</original>
    <translation xml:space="preserve">Go to the Temple of Purification and examine the marked locations.</translation>
  </message>
  <message>
    <key>q00000013_00_1496</key>
    <original xml:space="preserve">２Fにある制御室の鍵を探す</original>
    <translation xml:space="preserve">Find the key to the control room at 2F.</translation>
  </message>
  <message>
    <key>q00000013_00_1549</key>
    <original xml:space="preserve">鍵を使って制御室内から滝を止める</original>
    <translation xml:space="preserve">Turn off the flow of water from the control room.</translation>
  </message>
  <message>
    <key>q00000013_00_1497</key>
    <original xml:space="preserve">滝の先へ進む</original>
    <translation xml:space="preserve">Go to the location behind the waterfall.</translation>
  </message>
  <message>
    <key>q00000013_00_1000</key>
    <original xml:space="preserve">謎の誘拐犯を撃退する</original>
    <translation xml:space="preserve">Defeat the mysterious abductor.</translation>
  </message>
  <message>
    <key>q00000013_00_1001</key>
    <original xml:space="preserve">禊の神殿入口で合流する</original>
    <translation xml:space="preserve">Go to the entrance of the Temple of Purification.</translation>
  </message>
  <message>
    <key>q00000013_00_1002</key>
    <original xml:space="preserve">謁見の間のレオに経緯を報告する</original>
    <translation xml:space="preserve">Report back to Leo in the Audience Chamber.</translation>
  </message>
</resource>