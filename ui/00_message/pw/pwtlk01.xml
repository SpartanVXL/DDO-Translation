<resource endian="Little" locale="0" unknown="0" tag="TextWeb">
  <message>
    <original xml:space="preserve">よろしくお願いします</original>
    <translation xml:space="preserve">Nice to meet you!</translation>
  </message>
  <message>
    <original xml:space="preserve">ともに参りましょう</original>
    <translation xml:space="preserve">Let's go together!</translation>
  </message>
  <message>
    <original xml:space="preserve">お会いできて光栄です</original>
    <translation xml:space="preserve">It's an honor to meet you!</translation>
  </message>
  <message>
    <original xml:space="preserve">良き旅になるよう、全力を尽くします</original>
    <translation xml:space="preserve">I will do my best to make it a pleasant journey.</translation>
  </message>
  <message>
    <original xml:space="preserve">どうか、私の力を存分にお使いください</original>
    <translation xml:space="preserve">Please, use my powers to your liking.</translation>
  </message>
  <message>
    <original xml:space="preserve">我らが竜とともに！</original>
    <translation xml:space="preserve">The dragon is with us!</translation>
  </message>
  <message>
    <original xml:space="preserve">ご友人がお見えです</original>
    <translation xml:space="preserve">Your friend is visible.</translation>
  </message>
  <message>
    <original xml:space="preserve">覚者さま、お見事です</original>
    <translation xml:space="preserve">Good work, Arisen!</translation>
  </message>
  <message>
    <original xml:space="preserve">やりましたね！</original>
    <translation xml:space="preserve">We did it!</translation>
  </message>
  <message>
    <original xml:space="preserve">我が主に竜の加護があらんことを</original>
    <translation xml:space="preserve">Bless me with the Dragon's protection.</translation>
  </message>
  <message>
    <original xml:space="preserve">うまくいきませんね――</original>
    <translation xml:space="preserve">It's not going well.</translation>
  </message>
  <message>
    <original xml:space="preserve">いい手ごたえです</original>
    <translation xml:space="preserve">Good reaction.</translation>
  </message>
  <message>
    <original xml:space="preserve">力が湧いてきました！</original>
    <translation xml:space="preserve">My power is overflowing!</translation>
  </message>
  <message>
    <original xml:space="preserve">一層お役に立ってみせます！</original>
    <translation xml:space="preserve">I'll show you I can be useful!</translation>
  </message>
  <message>
    <original xml:space="preserve">さあ、行きましょう</original>
    <translation xml:space="preserve">Come on, let's go!</translation>
  </message>
  <message>
    <original xml:space="preserve">いよいよですね</original>
    <translation xml:space="preserve">Finally!</translation>
  </message>
  <message>
    <original xml:space="preserve">敵を発見！</original>
    <translation xml:space="preserve">Enemy sighted!</translation>
  </message>
  <message>
    <original xml:space="preserve">あそこに敵がいます！</original>
    <translation xml:space="preserve">There's an enemy there!</translation>
  </message>
  <message>
    <original xml:space="preserve">いました、ご注意を！</original>
    <translation xml:space="preserve">There! Careful!</translation>
  </message>
  <message>
    <original xml:space="preserve">あの敵は、まずいですね――</original>
    <translation xml:space="preserve">That enemy is disgusting...</translation>
  </message>
  <message>
    <original xml:space="preserve">強敵です、警戒を！</original>
    <translation xml:space="preserve">A formidable foe! Stay vigilant!</translation>
  </message>
  <message>
    <original xml:space="preserve">感謝します！</original>
    <translation xml:space="preserve">Thank you!</translation>
  </message>
  <message>
    <original xml:space="preserve">早く、助けを！</original>
    <translation xml:space="preserve">Help me, quickly!</translation>
  </message>
  <message>
    <original xml:space="preserve">助けてください！</original>
    <translation xml:space="preserve">Please help me!</translation>
  </message>
  <message>
    <original xml:space="preserve">し、しくじりました――！</original>
    <translation xml:space="preserve">I...I messed up!</translation>
  </message>
  <message>
    <original xml:space="preserve">くっ――力が――！</original>
    <translation xml:space="preserve">Ugh...my strength!</translation>
  </message>
  <message>
    <original xml:space="preserve">油断は禁物です！</original>
    <translation xml:space="preserve">Negligence is forbidden!</translation>
  </message>
  <message>
    <original xml:space="preserve">さあ、やりましょう！</original>
    <translation xml:space="preserve">Come on, let's do this!</translation>
  </message>
  <message>
    <original xml:space="preserve">倒しました！</original>
    <translation xml:space="preserve">Defeated!</translation>
  </message>
  <message>
    <original xml:space="preserve">我々の勝利です！</original>
    <translation xml:space="preserve">This is our victory!</translation>
  </message>
  <message>
    <original xml:space="preserve">生き延びたようです</original>
    <translation xml:space="preserve">Looks like we survived.</translation>
  </message>
  <message>
    <original xml:space="preserve">今です、覚者さま！</original>
    <translation xml:space="preserve">Do it now, Arisen!</translation>
  </message>
  <message>
    <original xml:space="preserve">まだ充分、戦えます</original>
    <translation xml:space="preserve">I still have plenty of fight left in me.</translation>
  </message>
  <message>
    <original xml:space="preserve">厳しい状況ですね</original>
    <translation xml:space="preserve">This is a tough situation.</translation>
  </message>
  <message>
    <original xml:space="preserve">このままでは不利かもしれません</original>
    <translation xml:space="preserve">At this rate we will be at a disadvantage!</translation>
  </message>
  <message>
    <original xml:space="preserve">深手を――負いましたっ――</original>
    <translation xml:space="preserve">I've suffered...a grevious wound.</translation>
  </message>
  <message>
    <original xml:space="preserve">体力が、もう――！</original>
    <translation xml:space="preserve">I'm out of stamina!</translation>
  </message>
  <message>
    <original xml:space="preserve">そんな、まさか――！</original>
    <translation xml:space="preserve">This can't be！</translation>
  </message>
  <message>
    <original xml:space="preserve">これでっ――！</original>
    <translation xml:space="preserve">Like this!</translation>
  </message>
  <message>
    <original xml:space="preserve">回復します</original>
    <translation xml:space="preserve">I'm recovering.</translation>
  </message>
  <message>
    <original xml:space="preserve">敵を引きつけます！</original>
    <translation xml:space="preserve">I will attract the enemy!</translation>
  </message>
  <message>
    <original xml:space="preserve">これで有利になるはずです</original>
    <translation xml:space="preserve">This should be to our advantage now.</translation>
  </message>
  <message>
    <original xml:space="preserve">回避します！</original>
    <translation xml:space="preserve">Evading!</translation>
  </message>
  <message>
    <original xml:space="preserve">喰らえ！</original>
    <translation xml:space="preserve">Eat this!</translation>
  </message>
  <message>
    <original xml:space="preserve">狙います！</original>
    <translation xml:space="preserve">Aiming!</translation>
  </message>
  <message>
    <original xml:space="preserve">逃さない！</original>
    <translation xml:space="preserve">I won't let go!</translation>
  </message>
  <message>
    <original xml:space="preserve">こっちです！</original>
    <translation xml:space="preserve">Over here!</translation>
  </message>
  <message>
    <original xml:space="preserve">はい！</original>
    <translation xml:space="preserve">Yes!</translation>
  </message>
  <message>
    <original xml:space="preserve">承知しました！</original>
    <translation xml:space="preserve">Acknowledged!</translation>
  </message>
  <message>
    <original xml:space="preserve">従えません</original>
    <translation xml:space="preserve">I won't follow.</translation>
  </message>
</resource>