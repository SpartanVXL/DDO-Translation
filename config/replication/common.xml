<replication>
  <message_set name="job name">
    <match>^ui\\00_message\\common\\job_name$</match>
  </message_set>
  <message_set name="npc name">
    <match>^ui\\00_message\\npc\\npc_name$</match>
    <match>^ui\\00_message\\enemy\\enemy_name$</match>
  </message_set>
  <message_set name="npc class">
    <match>^ui\\00_message\\npc\\npc_class_name$</match>
    <match>^ui\\00_message\\npc\\func_class_name$</match>
    <match>^ui\\00_message\\npc\\func_select_name$</match>
  </message_set>
  <message_set name="ability name">
    <match>^ui\\00_message\\skill\\ability_name$</match>
  </message_set>
  <message_set name="normal skill name">
    <match>^ui\\00_message\\skill\\normal_skill_name_\d+$</match>
    <extra>
      <original>爆矢射ち</original>
      <translation>Explosive Arrow</translation>
    </extra>
  </message_set>
  <message_set name="arisen card">
    <match>^ui\\00_message\\ui\\ArisenCard$</match>
    <match>^ui\\00_message\\ui\\ArisenCard_win$</match>
  </message_set>
  <message_set name="arisen card background name">
    <match>^ui\\00_message\\ui\\arisencard_background_name$</match>
  </message_set>
</replication>