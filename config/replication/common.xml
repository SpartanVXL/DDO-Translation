      <original>爆矢射ち</original>
      <translation>Explosive Arrow</translation>
    </extra>
  </message_set>
  <message_set name="arisen card">
    <match>^ui\\00_message\\ui\\ArisenCard$</match>
    <match>^ui\\00_message\\ui\\ArisenCard_win$</match>
  </message_set>
  <message_set name="arisen card background name">
    <match>^ui\\00_message\\ui\\arisencard_background_name$</match>
  </message_set>
