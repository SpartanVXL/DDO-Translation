<replication>
  <message_set name="job name">
    <match>^ui\\00_message\\common\\job_name$</match>
    <extra>
      <original>エレメントアーチャ―</original>
      <translation>Element Archer</translation>
    </extra>
    <extra>
      <original>Ｅアーチャー</original>
      <translation>Element Archer</translation>
    </extra>
    <extra>
      <original>Ｓセージ</original>
      <translation>Shield Sage</translation>
    </extra>
    <extra>
      <original>Ｓランサー</original>
      <translation>Spirit Lancer</translation>
    </extra>
  </message_set>
  <message_set name="ability name">
    <match>^ui\\00_message\\skill\\ability_name$</match>
  </message_set>
  <message_set name="normal skill name">
    <match>^ui\\00_message\\skill\\normal_skill_name_\d+$</match>
